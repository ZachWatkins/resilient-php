{
    "name": "zacharywatkins/resilient-framework-php",
    "description": "A template repository for resilient PHP applications",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Zachary K. Watkins",
            "email": "3458875+ZachWatkins@users.noreply.github.com",
            "homepage": "https://github.com/zachwatkins/",
            "role": "Creator"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/zachwatkins/resilient-framework-php/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/zachwatkins/resilient-framework-php/issues",
        "source": "https://github.com/zachwatkins/resilient-framework-php/",
        "docs": "https://raw.githubusercontent.com/zachwatkins/resilient-framework-php/main/README.md"
    },
    "require": {
        "php": ">=8.1",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "serve": "php -a"
    },
    "scripts-descriptions": {
        "serve": "Run a local PHP server and open your application in a browser window."
    }
}
